{
  "routes": [
    {
      "src": "/",
      "dest": "/quiz-simple.html"
    },
    {
      "src": "/quiz",
      "dest": "/quiz-simple.html"
    },
    {
      "src": "/quiz-simple.html",
      "dest": "/quiz-simple.html"
    }
  ],
  "functions": {},
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
